{
  "name": "filepicker-js",
  "version": "2.1.0",
  "description": "Filepicker client javascript library",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-src": "uglifyjs ./src/setup.js ./src/**/*.js ./src/finalize.js -o ./dist/filepicker.js -b",
    "build-min": "uglifyjs ./src/setup.js ./src/**/*.js ./src/finalize.js -o ./dist/filepicker.min.js --compress --mangle",
    "prebuild":"rm -rf dist && mkdir dist",
    "postbuild":"cp filepicker_debug.js dist/",
    "build": "npm run build-src && npm run build-min",
    "watch": "onchange ./src/*.js ./src/**/*.js -- npm run build && npm run jshint",
    "linter": "jshint ./src/* ./src/**/*",
    "watch-linter": "onchange ./src/*.js ./src/**/*.js -- npm run linter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filepicker/filepicker-js.git"
  },
  "keywords": [
    "filepicker",
    "file",
    "upload",
    "file",
    "processing",
    "store",
    "widget"
  ],
  "author": "krystiangw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filepicker/filepicker-js/issues"
  },
  "homepage": "https://github.com/filepicker/filepicker-js",
  "devDependencies": {
    "jshint": "^2.8.0",
    "onchange": "^2.0.0",
    "uglify-js": "^2.4.24"
  },
  "dependencies": {}
}
